"Window[platform=android]": {
	navBarHidden: "true"
},
".activityIndicatorWhite":{
	color: "#5da423",
	top: 0,
	height: "auto",
	width: "auto"
},
".activityIndicatorWhite[platform=ios]":{
	font: {fontSize: 15}
},
".activityIndicatorWhite[platform=android]":{
	font: {fontSize: "20dp"}
},
".customStatusBar":{
	backgroundColor: "#5da423"
},
".customStatusBar[platform=android]":{
	height: 80
},
".customStatusBar[platform=ios]":{
	height: 25
},
".container": {
	backgroundColor:"#fff"
},
".containerScrollView":{
	height: "100%",
	width: "100%"
},
".customStatusBarLeftButton":{
	background: "#5da423",
	image: "/images/haypista_symbol.png",
	color: "#fff"
},
".customStatusBarLeftButton[platform=android]":{
	left: 5,
	width: 50
},
".customStatusBarLeftButton[platform=ios]":{
	left: 2,
	width: 20
},
".customStatusBarTitle[platform=android]":{

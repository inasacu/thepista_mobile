	font: { fontSize:"20dp"}
},
".customStatusBarTitle[platform=ios]":{
	color:"#fff",
	font: { fontSize:15}
},
".customStatusBarTitleImage[platform=android]":{
	image:"/images/haypista_logo.png",
	width: "250"
},
".customStatusBarTitleImage[platform=ios]":{
	image:"/images/haypista_logo.png",
	width: "70"
},
".windowTitleWrapper":{
	height: 21,
	top: 0
},
".windowTitle":{
	backgroundColor: "#fff",
	height: 20
},
".windowTitleLabel": {
